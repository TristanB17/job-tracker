All Jobs for <strong><%= @company.name %></strong>

<ul>
  <% @company.jobs.each do |job| %>
    <li><%= job.description %></li>
    <li><%= link_to "Edit", edit_job(job) %> </li>
    <li><%= link_to "Delete", job(job), method: 'delete', id: "delete-#{job.id}" %> </li>
  <% end %>
</ul>
<h2>Company contacts: </h2>
<ul>
<% @company.contacts.each do |contact| %>
    <li><%= contact.name %></li>
    <li><%= contact.position %></li>
    <li><%= contact.email %></li>
<% end %>
</ul>
